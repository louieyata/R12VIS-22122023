@model IEnumerable<R12VIS.Models.Province>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Region.RegionName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.province_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.province_code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.province_code_excel)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Region.RegionName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.province_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.province_code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.province_code_excel)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.province_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.province_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.province_id })
        </td>
    </tr>
}

</table>
